{
    "extends": ["airbnb", "prettier"],
    "plugins": ["prettier"],
    "env": {
      "browser": true,
      "es6": true
    },
    "rules": {
      "no-alert": "off",
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "semi": false,
          "printWidth": 80,
          "tabWidth": 2,
          "endOfLine": "auto"
        }
      ]
    },
    "parserOptions": {
      "ecmaVersion": "latest"
  }
  }
  